{
    "name": "dinhsan/laravel-mautic-api",
    "description": "Free and Open Source Marketing Automation API",
    "license": "MIT",
    "keywords": ["mautic", "laravel", "laravel 9", "api", "sync", "automation", "email", "marketing"],
    "authors": [{
        "name": "Dinh San",
        "email": "dinhsan200@gmail.com"
    }],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.0.2",
        "laravel/framework": "^9.0",
        "mautic/api-library": "^3.1.0",
        "guzzlehttp/guzzle": "^7.5.0",
        "graham-campbell/manager": "^4.7.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Triibo\\Mautic\\MauticServiceProvider"
            ],
            "aliases": {
                "Mautic": "Triibo\\Mautic\\Facades\\Mautic"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Triibo\\Mautic\\": "src/"
        }
    }
}
